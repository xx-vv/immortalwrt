/dts-v1/;

#include "ipq5018-mx-base.dtsi"

/ {
	model = "Linksys MX2000";
	compatible = "linksys,mx2000", "qcom,ipq5018";
};

&q6v5_wcss {
	status = "okay";
	boot-args = <0x01 0x04 0x02 0x0f 0x00 0x00>;
};

&wifi0 {
	status = "okay";
	qcom,ath11k-calibration-variant = "Linksys-MX2000";
};

&wifi1 {
	status = "okay";
	qcom,ath11k-calibration-variant = "Linksys-MX2000";
};
